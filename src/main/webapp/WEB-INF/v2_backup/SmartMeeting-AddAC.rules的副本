@prefix fa: <http://www.semanticweb.org/SmartMeeting#>.@include <RDFS>.@include <OWL>.
[rule1: (?a rdf:type fa:MeetingRoom)(?a fa:hasSensor ?c)(?c rdf:type fa:LightSensor)(?b rdf:type fa:LightIntensity)(?b fa:measuredBy ?c)(?b fa:hasValue ?e) stringToInt(?e,?f) le(?f,100 )(?a fa:hasState ?d)(?d rdf:type fa:LightState)->(?d fa:hasValue 'weakLight')]
[rule2: (?a rdf:type fa:MeetingRoom)(?a fa:hasSensor ?c)(?c rdf:type fa:LightSensor)(?b rdf:type fa:LightIntensity)(?b fa:measuredBy ?c)(?b fa:hasValue ?e) stringToInt(?e,?f) ge(?f,300 )(?a fa:hasState ?d)(?d rdf:type fa:LightState)->(?d fa:hasValue 'hardLight')]
[rule3: (?a rdf:type fa:MeetingRoom)(?a fa:hasSensor ?c)(?c rdf:type fa:LightSensor)(?b rdf:type fa:LightIntensity)(?b fa:measuredBy ?c)(?b fa:hasValue ?e) stringToInt(?e,?f) lessThan(?f,300 )greaterThan(?f,100 )(?a fa:hasState ?d)(?d rdf:type fa:LightState)->(?d fa:hasValue 'suitable')]
[rule4: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:LightState)(?b fa:hasValue 'weakLight')(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartLight)(?d fa:hasValue '0')->(?c fa:hasValue 'open')]
[rule5: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:LightState)(?b fa:hasValue 'weakLight')(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartLight)(?d fa:hasValue '1')->(?c fa:hasValue 'undo')]
[rule6: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:LightState)(?b fa:hasValue 'hardLight')(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartLight)(?d fa:hasValue '0')->(?c fa:hasValue 'undo')]
[rule7: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:LightState)(?b fa:hasValue 'hardLight')(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartLight)(?d fa:hasValue '1')->(?c fa:hasValue 'off')]
[rule8: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:LightState)(?b fa:hasValue 'suitable')(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)->(?c fa:hasValue 'undo')]
[rule9: (?a rdf:type fa:MeetingRoom)(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)(?c fa:hasValue 'off')(?d fa:controlledBy ?c)(?d rdf:type fa:SmartLight)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/0',?g)->(?e fa:hasServiceURL ?g)]
[rule10: (?a rdf:type fa:MeetingRoom)(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)(?c fa:hasValue 'open')(?d fa:controlledBy ?c)(?d rdf:type fa:SmartLight)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/1',?g)->(?e fa:hasServiceURL ?g)]
[rule11: (?a rdf:type fa:MeetingRoom)(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)(?c fa:hasValue 'undo')(?d fa:controlledBy ?c)(?d rdf:type fa:SmartLight)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'',?g)->(?e fa:hasServiceURL ?g)]
[rule12: (?a rdf:type fa:MeetingRoom)(?a fa:hasSmartDevice ?d)(?d rdf:type fa:SmartLight)(?d fa:hasService ?e)(?e fa:hasServiceType 'get')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'',?g)->(?e fa:hasServiceURL ?g)]
[rule13: (?a rdf:type fa:MeetingRoom)(?a fa:hasSensor ?d)(?d rdf:type fa:LightSensor)(?d fa:hasService ?e)(?e fa:hasServiceType 'get')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'',?g)->(?e fa:hasServiceURL ?g)]
[rule14: (?a rdf:type fa:MeetingRoom)(?a fa:hasSensor ?c)(?c rdf:type fa:TemperatureSensor)(?b rdf:type fa:Temperature)(?b fa:measuredBy ?c)(?b fa:hasValue ?e) stringToInt(?e,?f) le(?f,24 )(?a fa:hasState ?d)(?d rdf:type fa:TemperatureState)->(?d fa:hasValue 'cold')]
[rule15: (?a rdf:type fa:MeetingRoom)(?a fa:hasSensor ?c)(?c rdf:type fa:TemperatureSensor)(?b rdf:type fa:Temperature)(?b fa:measuredBy ?c)(?b fa:hasValue ?e) stringToInt(?e,?f) ge(?f,26)(?a fa:hasState ?d)(?d rdf:type fa:TemperatureState)->(?d fa:hasValue 'hot')]
[rule16: (?a rdf:type fa:MeetingRoom)(?a fa:hasSensor ?c)(?c rdf:type fa:TemperatureSensor)(?b rdf:type fa:Temperature)(?b fa:measuredBy ?c)(?b fa:hasValue ?e) stringToInt(?e,?f) lessThan(?f,24 )greaterThan(?f,26 )(?a fa:hasState ?d)(?d rdf:type fa:TemperatureState)->(?d fa:hasValue 'suitable')]
[rule17: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:TemperatureState)(?b fa:hasValue 'cold')(?a fa:hasEvent ?c)(?c rdf:type fa:AirConditionerEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartAirConditioner)(?d fa:hasValue '0')->(?c fa:hasValue 'heat')]
[rule18: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:TemperatureState)(?b fa:hasValue 'cold')(?a fa:hasEvent ?c)(?c rdf:type fa:AirConditionerEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartAirConditioner)(?d fa:hasValue '1')->(?c fa:hasValue 'heat')]
[rule19: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:TemperatureState)(?b fa:hasValue 'cold')(?a fa:hasEvent ?c)(?c rdf:type fa:AirConditionerEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartAirConditioner)(?d fa:hasValue '2')->(?c fa:hasValue 'undo')]
[rule20: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:TemperatureState)(?b fa:hasValue 'hot')(?a fa:hasEvent ?c)(?c rdf:type fa:AirConditionerEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartAirConditioner)(?d fa:hasValue '0')->(?c fa:hasValue 'cool')]
[rule21: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:TemperatureState)(?b fa:hasValue 'hot')(?a fa:hasEvent ?c)(?c rdf:type fa:AirConditionerEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartAirConditioner)(?d fa:hasValue '1')->(?c fa:hasValue 'undo')]
[rule22: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:TemperatureState)(?b fa:hasValue 'hot')(?a fa:hasEvent ?c)(?c rdf:type fa:AirConditionerEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartAirConditioner)(?d fa:hasValue '2')->(?c fa:hasValue 'heat')]
[rule23: (?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:TemperatureState)(?b fa:hasValue 'suitable')(?a fa:hasEvent ?c)(?c rdf:type fa:AirConditionerEvent)->(?c fa:hasValue 'off')]
[rule24: (?a rdf:type fa:MeetingRoom)(?a fa:hasEvent ?c)(?c rdf:type fa:AirConditionerEvent)(?c fa:hasValue 'cool')(?d fa:controlledBy ?c)(?d rdf:type fa:SmartAirConditioner)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/1',?g)->(?e fa:hasServiceURL ?g)]
[rule25: (?a rdf:type fa:MeetingRoom)(?a fa:hasEvent ?c)(?c rdf:type fa:AirConditionerEvent)(?c fa:hasValue 'heat')(?d fa:controlledBy ?c)(?d rdf:type fa:SmartAirConditioner)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/2',?g)->(?e fa:hasServiceURL ?g)]
[rule26: (?a rdf:type fa:MeetingRoom)(?a fa:hasEvent ?c)(?c rdf:type fa:AirConditionerEvent)(?c fa:hasValue 'undo')(?d fa:controlledBy ?c)(?d rdf:type fa:SmartAirConditioner)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'',?g)->(?e fa:hasServiceURL ?g)]
[rule27: (?a rdf:type fa:MeetingRoom)(?a fa:hasEvent ?c)(?c rdf:type fa:AirConditionerEvent)(?c fa:hasValue 'off')(?d fa:controlledBy ?c)(?d rdf:type fa:SmartAirConditioner)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/0',?g)->(?e fa:hasServiceURL ?g)]
[rule28: (?a rdf:type fa:MeetingRoom)(?a fa:hasSmartDevice ?d)(?d rdf:type fa:SmartAirConditioner)(?d fa:hasService ?e)(?e fa:hasServiceType 'get')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'',?g)->(?e fa:hasServiceURL ?g)]
[rule29: (?a rdf:type fa:MeetingRoom)(?a fa:hasSensor ?d)(?d rdf:type fa:TemperatureSensor)(?d fa:hasService ?e)(?e fa:hasServiceType 'get')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'',?g)->(?e fa:hasServiceURL ?g)]
