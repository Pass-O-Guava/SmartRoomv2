@prefix fa: <http://www.semanticweb.org/SmartMeeting#>.@include <RDFS>.@include <OWL>.
[rule1:(?a rdf:type fa:MeetingRoom)(?a fa:hasSensor ?c)(?c rdf:type fa:LightSensor)(?b rdf:type fa:LightIntensity)(?b fa:measuredBy ?c)(?b fa:hasValue ?e) stringToInt(?e,?f) le(?f,100 )(?a fa:hasState ?d)(?d rdf:type fa:LightState)->(?d fa:hasValue 'weakLight')]
[rule2:(?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:LightState)(?b fa:hasValue 'weakLight')(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartLight)->(?c fa:hasValue 'open')]
[rule3:(?a rdf:type fa:MeetingRoom)(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)(?c fa:hasValue 'open')(?d fa:controlledBy ?c)(?d rdf:type fa:SmartLight)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/1',?g)->(?e fa:hasServiceURL ?g)]
[rule4:(?a rdf:type fa:MeetingRoom)(?a fa:hasSensor ?c)(?c rdf:type fa:LightSensor)(?b rdf:type fa:LightIntensity)(?b fa:measuredBy ?c)(?b fa:hasValue ?e) stringToInt(?e,?f) greaterThan(?f,100 )(?a fa:hasState ?d)(?d rdf:type fa:LightState)->(?d fa:hasValue 'okLight')]
[rule5:(?a rdf:type fa:MeetingRoom)(?a fa:hasState ?b)(?b rdf:type fa:LightState)(?b fa:hasValue 'okLight')(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)(?d fa:controlledBy ?c)(?d rdf:type fa:SmartLight)->(?c fa:hasValue 'off')]
[rule6:(?a rdf:type fa:MeetingRoom)(?a fa:hasEvent ?c)(?c rdf:type fa:LightEvent)(?c fa:hasValue 'off')(?d fa:controlledBy ?c)(?d rdf:type fa:SmartLight)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/0',?g)->(?e fa:hasServiceURL ?g)]
[rule7:(?a rdf:type fa:Temperature)(?a fa:hasValue ?b) stringToInt(?b,?c) le(?c,24 )(?d rdf:type fa:SmartAirConditioner)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/2',?g)->(?e fa:hasServiceURL ?g)]
[rule8:(?a rdf:type fa:Temperature)(?a fa:hasValue ?b) stringToInt(?b,?c) ge(?c,24 )(?d rdf:type fa:SmartAirConditioner)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/1',?g)->(?e fa:hasServiceURL ?g)]
[rule9:(?a rdf:type fa:Temperature)(?a fa:hasValue ?b) stringToInt(?b,?c) equal(?c,24 )(?d rdf:type fa:SmartAirConditioner)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/0',?g)->(?e fa:hasServiceURL ?g)]
[rule10:(?a rdf:type fa:LightSensor)(?a fa:hasService ?b)(?b fa:hasServiceType 'get')(?b fa:hasServiceEndPoint ?c)strConcat(?c,'',?d)->(?b fa:hasServiceURL ?d)]
[rule11:(?a rdf:type fa:TemperatureSensor)(?a fa:hasService ?b)(?b fa:hasServiceType 'get')(?b fa:hasServiceEndPoint ?c)strConcat(?c,'',?d)->(?b fa:hasServiceURL ?d)]