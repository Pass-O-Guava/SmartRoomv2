[rule2:(?a rdf:type fa:LightIntensity)(?a fa:hasValue ?b) stringToInt(?b,?c) greaterThan(?c,100 )(?d rdf:type fa:SmartLight)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/0',?g)->(?e fa:hasServiceURL ?g)]
[rule3:(?a rdf:type fa:Temperature)(?a fa:hasValue ?b) stringToInt(?b,?c) lessThan(?c,24 )(?d rdf:type fa:SmartAirConditioner)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/2',?g)->(?e fa:hasServiceURL ?g)]
[rule4:(?a rdf:type fa:Temperature)(?a fa:hasValue ?b) stringToInt(?b,?c) greaterThan(?c,24 )(?d rdf:type fa:SmartAirConditioner)(?d fa:hasService ?e)(?e fa:hasServiceType 'set')(?e fa:hasServiceEndPoint ?f)strConcat(?f,'/1',?g)->(?e fa:hasServiceURL ?g)]
