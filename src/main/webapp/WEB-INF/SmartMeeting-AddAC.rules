[rule6:(?a rdf:type fa:LightSensor)(?a fa:hasService ?b)(?b fa:hasServiceType 'get')(?b fa:hasServiceEndPoint ?c)strConcat(?c,'',?d)->(?b fa:hasServiceURL ?d)]
[rule7:(?a rdf:type fa:TemperatureSensor)(?a fa:hasService ?b)(?b fa:hasServiceType 'get')(?b fa:hasServiceEndPoint ?c)strConcat(?c,'',?d)->(?b fa:hasServiceURL ?d)]